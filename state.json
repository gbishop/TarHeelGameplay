{
  "language": { "pattern": "^([a-z]{2,3})?$", "default": "en" },
  "category": { "pattern": "^([-A-Za-z]+)?$", "default": "" },
  "type": { "pattern": "^[TCO]?$", "default": "" },
  "audience": { "pattern": "^[CE]?$", "default": "E" },
  "skill": { "pattern": "^\\d+$", "default": "0" },
  "search": { "pattern": "", "default": "" },
  "pageColor": { "pattern": "^[f0]{3}$", "default": "fff" },
  "textColor": { "pattern": "^[f0]{3}$", "default": "000" },
  "voice": {
    "pattern": "^(silent|male|female|child|browser)$",
    "default": "silent"
  },
  "locale": { "pattern": "^[a-z]{2,3}$", "default": "en" },
  "favorites": { "pattern": "(^[AR]?\\d+(,+\\d+)*$)|(^$)", "default": "" },
  "fpage": { "pattern": "^\\d+$", "default": "1" },
  "page": { "pattern": "^\\d+$", "default": "1" },
  "findAnotherLink": { "pattern": "", "default": "/find/" },
  "debug": { "pattern": "^[01]$", "default": "0" },
  "escape": { "pattern": "^[01]$", "default": "0" },
  "randomize": { "pattern": "^[01]$", "default": "1" },
  "moveprompts": { "pattern": "^[01]$", "default": "0" },
  "speech": { "pattern": "^[01]$", "default": "1" },
  "hover": { "pattern": "^[0124]$", "default": "0" }
}
